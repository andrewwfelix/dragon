{
  "name": "dragon-dnd-app",
  "version": "1.0.0",
  "description": "D&D 5e application with React frontend and Node.js backend",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm run dev",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "dev": "npm start"
  },
  "keywords": [
    "dnd",
    "dungeons-dragons",
    "react",
    "nodejs",
    "supabase"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "react-markdown": "^10.1.0"
  }
}
